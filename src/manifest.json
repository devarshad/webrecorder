{
    "manifest_version": 2,

    "name": "Web recorder",
    "description": "Record web page activity for element locator.",
    "version": "0.0.0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/",
        "file://*/*:/",
        "storage"
    ]
}
